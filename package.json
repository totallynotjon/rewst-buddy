{
	"publisher": "JBramley",
	"name": "rewst-buddy",
	"displayName": "rewst-buddy",
	"description": "Buddy for Rewst (Templates)",
	"version": "0.14.1",
	"engines": {
		"vscode": "^1.107.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "git@github.com:totallynotjon/rewst-buddy.git"
	},
	"bugs": {
		"url": "https://github.com/totallynotjon/rewst-buddy/issues"
	},
	"license": "MIT",
	"homepage": "https://github.com/totallynotjon/rewst-buddy/blob/master/README.md",
	"keywords": [
		"rewst",
		"Rewst",
		"buddy"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"configuration": {
			"title": "Settings Editor Rewst-Buddy",
			"type": "object",
			"properties": {
				"rewst-buddy.regions": {
					"type": "array",
					"default": [
						{
							"name": "North America",
							"cookieName": "appSession",
							"graphqlUrl": "https://api.rewst.io/graphql",
							"loginUrl": "https://app.rewst.io"
						}
					],
					"description": "Regional configuration for different Rewst instances",
					"items": {
						"type": "object",
						"properties": {
							"name": {
								"type": "string",
								"description": "Display name for the region"
							},
							"cookieName": {
								"type": "string",
								"description": "Session cookie name for authentication"
							},
							"graphqlUrl": {
								"type": "string",
								"description": "GraphQL API endpoint URL"
							},
							"loginUrl": {
								"type": "string",
								"description": "Login page URL for the region"
							}
						},
						"required": [
							"name",
							"cookieName",
							"graphqlUrl",
							"loginUrl"
						]
					}
				},
				"rewst-buddy.enableSyncOnSave": {
					"type": "boolean",
					"default": true,
					"description": "Enable Sync on Save."
				}
			}
		},
		"commands": [
			{
				"command": "rewst-buddy.NewSession",
				"title": "Rewst Buddy: New Rewst Session",
				"icon": "$(plus)"
			},
			{
				"command": "rewst-buddy.CreateTemplate",
				"title": "Rewst Buddy: Create Template",
				"icon": "$(file-add)"
			},
			{
				"command": "rewst-buddy.SyncTemplate",
				"title": "Rewst Buddy: Sync Template",
				"icon": "$(sync)"
			},
			{
				"command": "rewst-buddy.LinkTemplateFromURL",
				"title": "Rewst Buddy: Link Template from URL",
				"icon": "$(link-external)"
			},
			{
				"command": "rewst-buddy.LinkTemplateInteractive",
				"title": "Rewst Buddy: Link Template",
				"icon": "$(link-external)"
			},
			{
				"command": "rewst-buddy.ClearSessions",
				"title": "Rewst Buddy: Clear Sessions"
			},
			{
				"command": "rewst-buddy.OpenTemplateFromURL",
				"title": "Rewst Buddy: Open Template from URL"
			},
			{
				"command": "rewst-buddy.OpenTemplateInteractive",
				"title": "Rewst Buddy: Open Template"
			},
			{
				"command": "rewst-buddy.ClearTemplateLinks",
				"title": "Rewst Buddy: Clear Template Links"
			}
		],
		"views": {},
		"submenus": [],
		"menus": {}
	},
	"scripts": {
		"format:pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
		"format:eslint": "eslint --fix src",
		"format": "npm-run-all -p format:*",
		"lint": "eslint src",
		"type-check": "tsc --noEmit",
		"codegen": "graphql-codegen --config src/codegen.ts",
		"check-types": "tsc --noEmit --watch",
		"test": "vscode-test",
		"build": "npm run package",
		"dev": "npm run watch",
		"watch": "NODE_ENV=development npm-run-all -p watch:*",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"watch:webpack": "NODE_ENV=development webpack --mode development --watch",
		"compile": "NODE_ENV=development webpack --mode development",
		"vscode:prepublish": "npm run package",
		"package": "NODE_ENV=production webpack --mode production --devtool hidden-source-map"
	},
	"devDependencies": {
		"@0no-co/graphqlsp": "^1.15.2",
		"@eslint/js": "^9.39.2",
		"@graphql-codegen/cli": "^6.1.0",
		"@graphql-codegen/client-preset": "^5.2.1",
		"@graphql-codegen/schema-ast": "^5.0.0",
		"@graphql-codegen/typescript": "^5.0.6",
		"@graphql-codegen/typescript-graphql-request": "^6.3.0",
		"@graphql-codegen/typescript-operations": "^5.0.6",
		"@parcel/watcher": "^2.5.1",
		"@stylistic/eslint-plugin": "^5.6.1",
		"@types/mocha": "^10.0.10",
		"@types/node": "^25.0.3",
		"@types/vscode": "^1.107.0",
		"@typescript-eslint/eslint-plugin": "^8.50.0",
		"@typescript-eslint/parser": "^8.50.0",
		"@vscode/test-cli": "^0.0.12",
		"@vscode/test-electron": "^2.5.2",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-import": "^2.32.0",
		"globals": "^16.5.0",
		"graphql": "^16.12.0",
		"graphql-request": "^7.4.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.7.4",
		"ts-loader": "^9.5.4",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.50.0",
		"url-pattern": "^1.0.3",
		"uuid": "^13.0.0",
		"webpack": "^5.104.0",
		"webpack-cli": "^6.0.1"
	}
}
